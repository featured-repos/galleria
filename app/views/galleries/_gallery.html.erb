<div class="container">
  <% @gallery.photos.each do |f| %>
    <div class="col-md-2">
      <div class="row">
        <%= link_to f.title %>
      </div>
      <div class="row">
        <%= link_to f do %>
        <%= image_tag f.size(:thumbnail) %>
        <% end %>
      </div>
    </div>
  <% end %>
</div>
