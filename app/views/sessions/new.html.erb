<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h1 class="login"> Log In </h1>
    </div>
  </div>
  <div class="row">
    <div class="col-md-offset-3 col-md-6">
      <%= simple_form_for :session, class: "form-horizontal" do |f| %>
        <%= f.input :username %>
        <%= f.input :password %>
        <%= f.button :submit, "Log in" %>
      <% end %>
    </div>
  </div>
</div>
